EAAA,KANA;AAOA,MAAA,aAAA,EAAA,CAPA;AAQA,MAAA,aAAA,EAAA;AARA,KAAA;AAUA,GAlBA;AAmBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA,KAAA,KAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,SAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,SAPA;AAQA,OATA,MASA;AACA,QAAA,KAAA,CAAA,wBAAA,CAAA;AACA;AACA,KAdA;AAeA,IAAA,KAfA,mBAeA;AACA,WAAA,KAAA,CAAA,QAAA,GAAA,EAAA;AACA,KAjBA;AAkBA,IAAA,QAlBA,sBAkBA;AACA,WAAA,aAAA;AACA;AApBA,GAnBA;AAyCA,EAAA,OAzCA,qBAyCA;AAAA;;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,YAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;;AACA,UAAA,CAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,IAAA;AACA;AACA,KANA;AAOA;AAjDA,CAAA","sourcesContent":["<template>\n  <div id=\"blast\">\n    <v-row justify=\"center\">\n      <v-col cols=\"11\" sm=\"4\">\n        <SeqField\n          v-model=\"blast.inputSeq\"\n          custom-label=\"Query sequence\"\n        />\n        <SelectDatabase\n          v-model=\"blast.db\"\n        />\n        <FormButtons @clear=\"clear\" @submit=\"submit\" id=\"buttons\" :disabled=\"this.buttonDisable\"/>\n      </v-col>\n      <!---blastResult--->\n      <v-col cols=\"7\" sm=\"1\"/>\n      <v-col cols=\"7\" sm=\"6\">\n         <v-row justify=\"center\" >\n          <v-col>\n            <v-card\n              max-height=\"750\"\n              class=\"overflow-y-auto\"\n              v-scroll.self=\"onScroll\"\n            >\n              <div v-if=\"visual\">\n                <v-card-text>\n                  <pre>{{blastResult}}</pre>\n                </v-card-text>\n              </div>\n            </v-card>\n          </v-col>\n         </v-row>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nimport SeqField from '../components/SeqField'\nimport SelectDatabase from '../components/Blast/SelectDatabase'\nimport FormButtons from '@/components/FormButtons'\n\nexport default {\n  name: 'Blast',\n  components: {\n    SeqField,\n    SelectDatabase,\n    FormButtons\n  },\n  data() {\n    return {\n      blast: {\n        inputSeq: '',\n        db: 'TAIR10_Whole_Genome'\n      },\n      blastResult: '',\n      visual: false,\n      scrollInvoked: 0,\n      buttonDisable: false\n    }\n  },\n  methods: {\n    submit() {\n      if (this.blast.inputSeq) {\n        axios.post('/api/blast', this.blast)\n          .then(response => {\n            this.blastResult = response.data.result\n            this.visual = true\n          })\n          .catch(error => {\n            console.log(error.response.data)\n          })\n      } else {\n        alert('Fill in Sequence form.')\n      }\n    },\n    clear() {\n      this.blast.inputSeq = ''\n    },\n    onScroll() {\n      this.scrollInvoked++\n    }\n  },\n  mounted() {\n    axios.get('/api/blast')\n      .then(response => {\n        const judge = response.data.usable\n        if (!judge) {\n          this.buttonDisable = true\n        }\n      })\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}
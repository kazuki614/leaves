iBAAA,EAAA,KAAA,UAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,SAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA;AACA,SAPA;AAQA,OATA,MASA;AACA,QAAA,KAAA,CAAA,uCAAA,CAAA;AACA;AACA,KAdA;AAeA,IAAA,KAfA,mBAeA;AACA,WAAA,UAAA,CAAA,SAAA,GAAA,EAAA;AACA,WAAA,UAAA,CAAA,IAAA,GAAA,EAAA;AACA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AACA,WAAA,aAAA;AACA;AArBA;AAlBA,CAAA","sourcesContent":["<template>\n  <div id=\"conversion\">\n    <v-row justify=\"center\">\n        <v-col cols=\"11\" sm=\"4\">\n          <SeqField\n            v-model=\"conversion.input_seq\"\n            custom-label=\"Query sequence\"\n          />\n          <SelectMode\n            v-model=\"conversion.mode\"\n          />\n          <FormButtons @clear=\"clear\" @submit=\"submit\"/>\n        </v-col>\n        <!---Result--->\n        <v-col cols=\"7\" sm=\"1\"></v-col>\n        <v-col cols=\"7\" sm=\"6\">\n         <v-container\n            style=\"max-height: 700px\"\n            id=\"scroll-target\"\n            class=\"overflow-y-auto\">\n           <v-row justify=\"center\">\n            <v-col>\n              <v-card\n                v-scroll.self=\"onScroll\"\n                class=\"overflow-auto\">\n                <div v-if=\"visual\">\n                  <v-card-title>\n                    {{conversion.mode}}\n                  </v-card-title>\n                  <v-card-text>\n                    <pre>{{result}}</pre>\n                  </v-card-text>\n                </div>\n              </v-card>\n            </v-col>\n           </v-row>\n         </v-container>\n        </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nimport SeqField from '../components/SeqField'\nimport FormButtons from '@/components/FormButtons'\nimport SelectMode from '@/components/Conversation/SelectMode'\n\nexport default {\n  name: 'Conversion',\n  components: {\n    SeqField,\n    FormButtons,\n    SelectMode\n  },\n  data() {\n    return {\n      conversion: {\n        input_seq: '',\n        mode: ''\n      },\n      result: '',\n      visual: false,\n      scrollInvoked: 0\n    }\n  },\n  methods: {\n    submit() {\n      if (this.conversion.mode && this.conversion.input_seq) {\n        axios.post('/api/conversion', this.conversion)\n          .then(response => {\n            this.result = response.data.result\n            this.visual = true\n          })\n          .catch(error => {\n            console.log(error.response.data)\n          })\n      } else {\n        alert('Fill form of query sequence and mode.')\n      }\n    },\n    clear() {\n      this.conversion.input_seq = ''\n      this.conversion.mode = ''\n    },\n    onScroll() {\n      this.scrollInvoked++\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}